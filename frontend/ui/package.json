{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "atendeja-ops-ui",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "check": "npm run lint && npx tsc --noEmit && npm test && npm run build"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.4.8",
    "@testing-library/react": "14.3.1",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "4.3.1",
    "autoprefixer": "10.4.19",
    "eslint": "8.57.0",
    "eslint-plugin-react": "7.34.1",
    "jsdom": "24.1.0",
    "postcss": "8.4.39",
    "tailwindcss": "3.4.7",
    "typescript": "5.4.5",
    "vite": "5.2.10",
    "vitest": "1.6.0"
  },
  "vitest": {
    "test": {
      "environment": "jsdom",
      "setupFiles": [
        "./tests/setup.ts"
      ]
    }
  }
}
